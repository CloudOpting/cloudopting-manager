	elementFormDefault="qualified">


	<xs:complexType name="formData">
		<xs:attribute name="label"></xs:attribute>
	</xs:complexType>
	<xs:complexType name="tVMports">
		<xs:sequence>
			<xs:element name="VMport" minOccurs="0" maxOccurs="unbounded"
				type="xs:int" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="tlogtype">
		<xs:restriction base="xs:string">
			<xs:enumeration value="apache" />
			<xs:enumeration value="mysql" />
			<xs:enumeration value="postgresql" />
			<xs:enumeration value="tomcat" />
			<xs:enumeration value="mongodb" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tziptype">
		<xs:restriction base="xs:string">
			<xs:enumeration value="zip" />
			<xs:enumeration value="tar.gz" />
			<xs:enumeration value="tgz" />
			<xs:enumeration value="tgz2" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tcpu">
		<xs:restriction base="xs:int">
			<xs:enumeration value="1" />
			<xs:enumeration value="2" />
			<xs:enumeration value="4" />
			<xs:enumeration value="6" />
			<xs:enumeration value="8" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tdisk">
		<xs:restriction base="xs:int">
			<xs:enumeration value="5" />
			<xs:enumeration value="10" />
			<xs:enumeration value="15" />
			<xs:enumeration value="20" />
			<xs:enumeration value="30" />
			<xs:enumeration value="40" />
			<xs:enumeration value="50" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tram">
		<xs:restriction base="xs:int">
			<xs:enumeration value="512" />
			<xs:enumeration value="1024" />
			<xs:enumeration value="2048" />
			<xs:enumeration value="4096" />
			<xs:enumeration value="6144" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="tVMhostProperties">
		<xs:sequence>
			<xs:element name="cpu">
				<xs:complexType>
					<xs:simpleContent>
