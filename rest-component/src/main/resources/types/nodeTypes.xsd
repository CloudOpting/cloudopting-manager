		<xs:sequence>
			<xs:element name="source">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="title" fixed="Source of the remote file"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="destination">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="title" fixed="Destination of the remotefile"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="require">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="title" fixed="Name of a component that has to be installed before"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="before">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="title" fixed="Name of a component that has to be installed after"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="unzip">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:boolean">
							<xs:attribute name="title" fixed="doyou need to uncompress?"></xs:attribute>
							<xs:attribute name="formtype" fixed="boolean"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="target">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="title" fixed="Destination folder"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="archive">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="title" fixed="Name of the archive"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="extension">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="tziptype">
							<xs:attribute name="title" fixed="Extension of the archive"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
							<xs:attribute name="enum"
								fixed="['zip','tar.gz','tgz','tgz2']"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="archive_user">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="title" fixed="User for the uncompressed content"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="root_dir">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="title" fixed="Root folder name"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
							<xs:attribute name="description" fixed="enclose between single qoute"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RemotefileProperties" type="tRemotefileProperties" />
	<xs:complexType name="tSLA">
		<xs:sequence>
			<xs:element default="1" name="NumCpus">
				<xs:annotation>
					<xs:documentation xml:lang="en">Number of CPUs: Number
						of CPUs for the virtual machine
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="1" />
						<xs:enumeration value="2" />
						<xs:enumeration value="4" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Memory" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="en">Memory (in MB): Amount
						of memory for the virtual machine
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Price" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="en">Price for this SLA
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Disk">
				<xs:annotation>
					<xs:documentation xml:lang="en">Disk (in GB): Amount
						of
						disk for the virtual machine
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:enumeration value="5" />
						<xs:enumeration value="10" />
						<xs:enumeration value="15" />
						<xs:enumeration value="20" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Chosen" type="xs:boolean">
				<xs:annotation>
					<xs:documentation xml:lang="en">If the SLA is chosen
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Name" type="xs:string" use="required" />
		<xs:attribute name="id" type="xs:ID" use="required" />
	</xs:complexType>
	<xs:complexType name="tSLAs">
		<xs:sequence>
			<xs:element name="SLA" minOccurs="1" maxOccurs="unbounded"
				type="tSLA" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="tCustomizationProperty">
		<xs:sequence>
			<xs:element name="Name1" type="xs:string" minOccurs="1"
				fixed="pippo" />
			<xs:element name="Value" type="xs:string" minOccurs="1" />
		</xs:sequence>
		<xs:attribute name="Name2" type="xs:string" use="required" />
	</xs:complexType>
	<xs:element name="CustomizationProperty" type="tCustomizationProperty" />
	<xs:complexType name="tDockerPortProperty">
		<xs:attribute name="Port" type="xs:string" use="required" />
	</xs:complexType>
	<xs:element name="DockerPortProperty" type="tDockerPortProperty" />
	<xs:complexType name="tDockerVolumeProperty">
		<xs:attribute name="Src" type="xs:string" use="required" />
		<xs:attribute name="Dest" type="xs:string" use="optional" />
	</xs:complexType>
	<xs:element name="DockerVolumeProperty" type="tDockerVolumeProperty" />
	<xs:complexType name="tvolumeFromProperties">
		<xs:sequence>
			<xs:element name="source">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="title" fixed="Source"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="destination">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="title" fixed="Destination"></xs:attribute>
							<xs:attribute name="formtype" fixed="string"></xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="volumeFromProperties" type="tvolumeFromProperties" />



</xs:schema>
